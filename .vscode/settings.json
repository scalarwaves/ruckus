    "cmake.autoRestartBuild": true,
    "cmake.buildDirectory": "build",
    "cmake.clearOutputBeforeBuild": true,
    "cmake.cmakePath": "/home/dark/.asdf/shims/cmake",
    "cmake.ctest.parallelJobs": 8,
    "cmake.defaultVariants": {
        "buildType": {
            "default": "debug",
            "description": "The build type",
            "choices": {
                "debug": {
                    "short": "Debug",
                    "long": "Emit debug information without performing optimizations",
                    "buildType": "Debug"
                },
                "release": {
                    "short": "Release",
                    "long": "Enable optimizations, omit debug info",
                    "buildType": "Release"
                },
                "minsize": {
                    "short": "MinSizeRel",
                    "long": "Optimize for smallest binary size",
                    "buildType": "MinSizeRel"
                },
                "reldeb": {
                    "short": "RelWithDebInfo",
                    "long": "Perform optimizations AND include debugging information",
                    "buildType": "RelWithDebInfo"
                }
            }
        }
    },
    "cmake.enabledOutputParsers": [
        "cmake",
        "gcc",
        "gnuld",
        "msvc",
        "ghs"
    ],
    "cmake.generator": "Ninja",
    "cmake.installPrefix": "/usr/local/bin",
    "cmake.parallelJobs": 8,
    "cmake.saveBeforeBuild": true,
    "cmake.toolset": "Debug",
    "cmake.configureOnOpen": false,
    "code-runner.executorMap": {
        "ahk": "autohotkey",
        "applescript": "osascript",
        "autoit": "autoit3",
        "bat": "cmd /c",
        "c": "cd $dir && gcc $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "clojure": "lein exec",
        "coffeescript": "coffee",
        "cpp": "cd $dir && g++ $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "crystal": "crystal",
        "csharp": "scriptcs",
        "d": "cd $dir && dmd $fileName && $dir$fileNameWithoutExt",
        "dart": "dart",
        "fsharp": "fsi",
        "go": "go run",
        "groovy": "groovy",
        "haskell": "runhaskell",
        "haxe": "haxe --cwd $dirWithoutTrailingSlash --run $fileNameWithoutExt",
        "java": "cd $dir && javac $fileName && java $fileNameWithoutExt",
        "javascript": "node",
        "julia": "julia",
        "lisp": "sbcl --script",
        "lua": "lua",
        "nim": "nim compile --verbosity:0 --hints:off --run",
        "objective-c": "cd $dir && gcc -framework Cocoa $fileName -o $fileNameWithoutExt && $dir$fileNameWithoutExt",
        "ocaml": "ocaml",
        "pascal": "cd $dir && fpc $fileName && $dir$fileNameWithoutExt",
        "perl": "perl",
        "perl6": "perl6",
        "php": "php",
        "powershell": "powershell -ExecutionPolicy ByPass -File",
        "python": "python -u",
        "r": "Rscript",
        "racket": "racket",
        "ruby": "ruby",
        "rust": "cd $dir && rustc $fileName && $dir$fileNameWithoutExt",
        "scala": "scala",
        "shellscript": "bash",
        "swift": "swift",
        "typescript": "ts-node",
        "vbscript": "cscript //Nologo",
    },
    "code-runner.clearPreviousOutput": true,
    "code-runner.defaultLanguage": "cpp",
    "code-runner.enableAppInsights": false,
    "code-runner.preserveFocus": true,
    "code-runner.respectShebang": true,
    "code-runner.runInTerminal": true,
    "code-runner.saveAllFilesBeforeRun": true,
    "code-runner.showExecutionMessage": true,
    "code-runner.showRunIconInEditorTitleMenu": true,
    "code-runner.temporaryFileName": "tempCodeRunnerFile",
    "code-runner.showRunCommandInEditorContextMenu": true,
    "crates.upToDateDecorator": "âœ”",
    "rust.all_features": true,
    "rust.all_targets": false,
    "rust.build_lib": true,
    "rust.build_on_save": true,
    "rust.cfg_test": true,
    "rust.clear_env_rust_log": true,
    "rust.clippy_preference": "opt-in",
    // "rust.goto_def_racer_fallback": true,
    "rust.jobs": 8,
    "rust.rustflags": null,
    "rust.rustfmt_path": "/home/dark/.asdf/shims/rustfmt",
    "rust.show_warnings": false,
    "rust.target": "x86_64-unknown-linux-gnu",
    "rust.unstable_features": true,
    "rust.use_crate_blacklist": true,
    "rust.wait_to_build": 3000,
    "rust.racer_completion": true
}